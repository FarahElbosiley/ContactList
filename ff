void MainWindow::closeEvent(QCloseEvent *event)
{
    qDebug() << "Custom close event handling";

    // Call your custom function here
    myCustomFunction();

    // You can decide whether to accept or ignore the close event
    // For example, uncomment the line below to accept the close event
    // event->accept();

    // The default implementation of QMainWindow::closeEvent() accepts the close event
    QMainWindow::closeEvent(event);
}

